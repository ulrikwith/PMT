# Blue.cc API Credentials
BLUE_TOKEN_ID=your_token_id_here
BLUE_SECRET_ID=your_secret_id_here

# Blue.cc Todo List (shared in BYPASS_AUTH mode)
BLUE_TODO_LIST_ID=your_todo_list_id

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# WordPress / WooCommerce Webhook
WP_WEBHOOK_SECRET=changeme_set_this_in_woocommerce_webhook_settings

# Server Configuration
PORT=3001
NODE_ENV=development
APP_URL=http://localhost:3002

# Auth bypass: set to 'true' for local dev (only works when NODE_ENV != 'production')
BYPASS_AUTH=true
